    "prepare": "tsc",
    "prepublish": "tsc"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.17",
    "ngrok": "^4.3.1",
    "prettier": "^2.5.1",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "express": "^4.17.2"
  },
  "engines": {
    "node": "^16.13.2",
    "npm": "^8.1.2"
  }
}
